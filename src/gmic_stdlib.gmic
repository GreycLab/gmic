    angle+=3                                         # Update 3D angle.
    fps=${-fps} if $fps>0 to. $fps" fps",5,{h-22},16,1,0.2 fi
    w.
    if {*,CTRLLEFT}" && "{*,D} w[] {1.5*[w,h]} elif {*,CTRLLEFT}" && "{*,C} w[] {[w,h]} fi
    rm. wait 40
  while {*}" && "!{*,ESC}" && "!{*,Q}

