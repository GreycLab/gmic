fx_tones2layers :
  sval=$2 eval={max($2,$3)}
  remove_opacity
  foreach {
    nm={n}
    +luminance rv
    repeat $1-1 {
      [1]
      val0={$sval+($eval-$sval)*$>/($1-2)}
      val1={$sval+($eval-$sval)*($>+1)/($1-2)-1}
      +ir[0] $val0,$val1
      if $5 *. [0] b. $4% n. 0,255  # Scalar alpha.
      else b. $4% n. 0,255  # Binary alpha.
      fi
      a[-2,-1] c
    }
    rm[0] rv =>[^] $nm
  }

