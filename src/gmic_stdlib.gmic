  channels 0 f 0
  foreach {
    t=0
    repeat $3 {
      repeat $1 {
        r={$6+$5*cos(6*$7*$t)+(1-$5)*sin(6*$8*$t)}
        a={(360*sin($7*$t)+30*$6*$>)*pi/180}
        ax={2*$>*pi/$1+$t}
        cx={(1+$9*cos($ax)+$r*cos($a))*w/2}
        cy={(1+$9*sin($ax)+$r*sin($a))*h/2}
        ellipse. $cx,$cy,$2%,$2%,0,-1,1
      }
      t+={$4%}
    }
  }
  & 255 if $10 map {$10-1} fi

