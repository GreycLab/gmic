  ac "_fx_self_glitching ${1-7}",$8

_fx_self_glitching :
  f "begin(
       shift = ([w,h]-1)*(50-[$5,$6])%;
       const sign = $3?-1:1;
       const boundary = $7;
     );
     val = sign*((2^$1)*j(shift) + $2);
     ($4==0?(val + i):
      $4==1?(val * i):
      $4==2?(val & i):
      $4==3?(val | i):
      $4==4?xor(val,i):
      $4==5?(val^i):
      $4==6?(i^val):
      $4==7?(val%i):
      (i%val)
     )%256;
  "

