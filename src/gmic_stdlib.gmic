  _vt100_i="\33[3m"         # Italic
  _vt100_m="\33[0;35;59m"   # Magenta
  _vt100_n="\33[0;0;0m"     # Normal
  _vt100_r="\33[0;31;59m"   # Red
  _vt100_s="\33[9m"         # Strikethrough
  _vt100_u="\33[4m"         # Underline

