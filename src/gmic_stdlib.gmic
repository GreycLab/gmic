      }
    fi

    if !$! $img gui_no_preview , fi
    c. 0,255 r2din. 200,200,2 r. 100%,100%,1,{min(s,4)} to_rgba. r. 200,200,1,4,0,0,0.5,0.5
    if {*} w. ${"fitscreen ."} fi
    o. $file_out rm.
