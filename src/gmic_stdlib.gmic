fx_custom_transform :
  to_rgba repeat $! {
    f. "$1"
    s. c a[-4--2] c f.. "$2"
    s.. c f[-4] "$3" f... "$4" f.. "$5" f. "$6"
    if $7==0 a[-4--1] c c. 0,255
    elif $7==1 a[-4--2] c n.. 0,255 c. 0,255 a[-2,-1] c
    else a[-4--1] c n. 0,255
    fi
  mv. 0 }

