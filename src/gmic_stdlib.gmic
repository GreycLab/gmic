fx_colormap :
  foreach {
    split_opacity to_rgb[0]
    if $1>=2                # Pre-defined colormap.
      index[0] {$1-2},$2,1
    elif $1==1              # Custom colormap.
      (${5-28}) z. 0,{3*$4-1}
      r. 3,{w/3},1,1,-1 permute. yzcx r. $3,1,1,3,3
      index[0] .,$2,1 rm.
    else                      # Adaptive colormap.
      autoindex[0] $3,$2,{if($3<=32,1,0)}
    fi
    a c
  }

