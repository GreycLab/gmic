#@cli negate : base_value : (no arg)
#@cli : Negate image values.
#@cli : Default value: 'base_value=(undefined)'.
#@cli : $ image.jpg +negate
negate : skip "${1=,}"
  if isnum("$*")
    e[0--3] "Negate values of image$?, according to base value $*."
    - {"$*"} * -1
  else
    e[0--3] "Negate values of image$?."
    repeat $! { -[$>] {$>,iM} } * -1
    if ['"$1"']!=',' noarg fi
  fi

