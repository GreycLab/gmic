fx_circle_transform :
  foreach {
    to_rgba r={dx=($3-$1)*(w-1)%;dy=($4-$2)*(h-1)%;norm(dx,dy)}
    if $7==0 cond="i(X,Y,z,c,$8,$9)"
    elif $7==1 cond="if(N<"$r",i(X,Y,z,c,$8,$9),i)"
    else cond="if(N>"$r",i(X,Y,z,c,$8,$9),i)"
    fi
    f "U = x - w*$1%;
       V = y - h*$2%;
       N = sqrt(U*U + V*V);
       Nr = N - "$r";
       X = x + $5*Nr*U/N;
       Y = y + $6*Nr*V/N;
       "$cond
  }

