fx_ghost :
  foreach {
    split_opacity l[0] {
      diffusiontensors. 1,1,$2,$3 eigen. compose_channels.. + sqrt.. n.. 1,100 ^.. $4 n.. 0,$1
      100%,100%
      eval.. "*
        ca = i(x,y,0,0);
        sa = i(x,y,0,1);
        N = i(#-3,x,y);
        x0 = x + N*ca;
        y0 = y + N*sa;
        x1 = x - N*ca;
        y1 = y - N*sa;
        polygon(#-1,2,x0,y0,x1,y1,-1,1);
        I"
      k. normalize_local $5,$6
      n 0,255 if $7 negate fi
    }
    a c
  }

