#@gui : note = note("<small>Author: <i>David TschumperlÃ©</i>.\nLatest Update: <i>2015/25/08</i>.</small>")
fx_vector_painting :
  foreach {
    split_opacity l[0] {
      +luminance b. {10-$1}%,1,1
      f. "dmax = -1; nmax = 0;
          for (n = 0, ++n<=8,
            p = arg(n,-1,0,1,-1,1,-1,0,1);
            q = arg(n,-1,-1,-1,0,0,1,1,1);
            d = (j(p,q,0,0,0,1) - i)^2;
            d>dmax?(dmax = d; nmax = n):nmax;
          )"
      blend shapeaverage
    }
    a c
  }

fx_vector_painting_preview :
  gui_split_preview "fx_vector_painting $*",${-3--1}

