#@cli med
#@cli : Compute the median of selected images.
#@cli : $ image.jpg sample lena,lion,square +med
med :
  e[^-1] "Compute median of image$?."
  if !$! return fi
  13,$! eval. "!x?copy(i(),[[',i#'],v2s(y,10,10)])" =. 0 discard. 0 str={t} rm.
  ${-max_whds},"med("$str")" k. => [med]

