  foreach {
    100%,100%,1,4 rm[0]
    repeat $7 {
      x0={max(0,min(w,$1+u(-$3,$3)))} y0={max(0,min(h,$2+u(-$3,$3)))}
      x1={max(0,min(w,$4+u(-$6,$6)))} y1={max(0,min(h,$5+u(-$6,$6)))}
      u0={u(0,100)} v0={u(0,100)} u1={u(0,100)} v1={u(0,100)}
