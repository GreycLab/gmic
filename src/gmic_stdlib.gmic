fx_solve_maze :
  repeat $! {
    +norm. >=. 50%
    if !$10 negate. fi
    *. 255 +b. $5% *.. 1e10 +[-2,-1]
    minimal_path. $1%,$2%,0,$3%,$4%,0 transpose.
    pointcloud. 0 dilate. $6 r. ..,..,1,1,0
    to_rgba.
    replace_color. 0,0,1,1,1,255,${7-9},255
    replace_color. 0,0,0,0,0,255,0,0,0,0
    ellipse. $1%,$2%,5,5,0,1,${7-9},255
    ellipse. $3%,$4%,5,5,0,1,${7-9},255
    rv[-2,-1]
  mv[-2,-1] 0 }

