fx_local_processing :
  com0="n 0,255"
  com1="equalize 256,0,255 n 0,255"
  com=${com$1}
  if $6 com="s c "$com" a c" fi
  foreach {
    size={round(max(8,max(w,h)*$3%))}
    +ac "at \""$com"\","$size","$size",1,$4%,$4%",$7
    if $5 +norm[0] bilateral[1] .,{$5/20}%,{2+$5/4} rm. fi
    blend alpha,{$2%}
  }

