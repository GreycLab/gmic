_fx_mess_with_bits :
  foreach {
    +l {
      b {($2/100)^2*100}% if $1 n 0,255 fi * $3 round
      # -> Here, images are 'ushort'-valued.
      if $4==1 f "for (k = res = 0, k<15, ++k, res|=((i>>k)&1)<<(15-k))"
