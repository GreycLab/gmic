fx_start_mix :
  if $1==1 +tones. 3 +[-2,-1] b[-2,-1] $2% ri[-2,-1] ... *. ... mv... $!
  elif $1==2 +tones. 3 +[-3,-1] b[-2,-1] $2% ri[-2,-1] ... *.. ... mv... $!
  elif $1==3 +tones. 3 +[-3,-2] b[-2,-1] $2% ri[-2,-1] ... *.. ... mv... $!
  fi

fx_end_mix :
  if $1==1 *[-3,-1] +[-2,-1]
  elif $1==2 *[-2,-1] +[-2,-1]
  elif $1==3 *[-2,-1] +[-2,-1]
  fi
  c 0,255

fx_mix_rgb :
  foreach {
    split_opacity rv to_rgb.
    fx_start_mix $10,$11
    -. 128 s. c
    *... $1 +... $2 b... $3%
    *.. $4 +.. $5 b.. $6%
    *. $7 +. $8 b. $9%
    a[-3--1] c +. 128 c. 0,255
    fx_end_mix $10
    if $!!=3 rv a c fi
  }

