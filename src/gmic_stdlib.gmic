fx_charcoal :
  foreach {
    split_opacity l[0] {
      compose_channels max
      w={w} h={h}
      if $5 r. 150%,150%,1,1,6 fi
      if $4 equalize. n. 0,255 fi
      sharpen {$1*3} cut 0,255
      if $6 +ir $7,$8 fi  # Add highlights layer if required.
      ir[0] $2,$3
      if !$15 ==[0] 0 fi
      -|
      +*[0] $10 +*[0] $11 *[0] $9
      a[-3--1] c replace_color 0,0,0,0,0,$12,$13,$14
      r $w,$h,1,100%,2
    }
    a c
  }

