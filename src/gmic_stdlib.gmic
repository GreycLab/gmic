_center3d :
  e[0--3] "Center 3D object$?."
  check3d foreach {
    if i[6]
      s3d r[2] 3,{2,h/3},1,1,-1 s[2] x
      -[2] {2,(iM+im)/2} -[3] {3,(iM+im)/2} -[4] {4,(iM+im)/2}
      a[2-4] x y[2] a y
